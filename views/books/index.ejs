<h2>Search Books</h2>
<form action="/books" method="GET">
    <div>
  <label>Title</label>
  <input type="text" name="title" value="<%= searchOptions.title %>">
  </div>
<div>
  <label>Publish After</label>
  <input type="date" name="pulishAfter" value="<%= searchOptions.publishAfter %>">
  </div>
<div>
  <label>Publish Before</label>
  <input type="date" name="pulishBefore" value="<%= searchOptions.publishBefore %>">
  </div>
  <button type="submit">Search</button>
</form>
<div>
<% books.forEach(book => { %>
    <img width="100" height="150" src="<%= book.coverImagePath %>" alt="Image of the book">
<% }) %>
</div>